{
  "name": "jupyterlab_toastify",
  "version": "1.2.0",
  "description": "Integrate 'react-toastify' nicely in JupyterLab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/fcollonval/jupyterlab_toastify",
  "bugs": {
    "url": "https://github.com/fcollonval/jupyterlab_toastify/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Frederic Collonval",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcollonval/jupyterlab_toastify.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "precommit": "pretty-quick --staged",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.16.0",
    "react-toastify": "^4.2.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "prettier": "1.14.2",
    "pretty-quick": "^1.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.1",
    "typescript": "~2.9.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
